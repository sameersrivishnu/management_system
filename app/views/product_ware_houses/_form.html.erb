<%= form_with(model: @product_ware_house, local: true) do |form| %>


  <div class="field">
    <%= form.label :low_item_threshold %>
    <%= form.text_field :low_item_threshold %>
  </div>
  <div class="field">
    <%= form.label :item_count %>
    <%= form.text_field :item_count %>
  </div>  
  <div class="actions">
    <%= form.submit class: 'btn btn-success' %>
  </div>
<% end %>
